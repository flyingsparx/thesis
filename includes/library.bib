	Date-Added = {2011-04-20 12:09:50 +0100},
	Date-Modified = {2011-06-06 13:20:08 +0100},
	Doi = {10.1109/ACSSC.2008.5074596},
	Issn = {1058-6393},
	Keywords = {data quality;diverse domains;information quality aware sensor network services;network data transport;network protocol;quality-of-service;sensor fusion algorithms;sensor sources;wirelessly inter-connected devices;diversity reception;protocols;quality of service;sensor fusion;wireless sensor networks;},
